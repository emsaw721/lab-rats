

<div class="lab-comment-list">
    {{#each this}}
    <section class="comment" data-commentseq="{{id}}">
        <div class="meta">
            {{user.username}} on {{format_date created_at}}
        </div>
        <div class="text">
            {{comment_text}}
        </div>
    </section>
    {{/each}}
</div>

